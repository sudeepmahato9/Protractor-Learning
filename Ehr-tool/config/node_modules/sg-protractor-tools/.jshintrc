{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "devel": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "quotmark": "single",
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "globalstrict": true,
    "trailing": true,
    "maxparams": 5,
    "maxcomplexity": 5,
    "jquery": true,
    "predef": [
        "sg", "sungard", "angular",
        "_", "window", "SunGard",
        "yepnope", "jsonString", "setTimeout",
        "clearTimeout", "inputs", "kendo",
        "define", "require",
        "describe", "beforeEach", "afterEach", "it", "expect",
        "module", "inject", "__dirname",
        "browser", "protractor",
        "element", "by"
    ]
}
